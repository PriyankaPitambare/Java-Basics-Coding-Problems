package com.learnjava.Practicejava;

import java.util.Scanner;

public class Learnjava {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//reverse number num =789 o/p=987	
		
		int rem;
		
		int num=789;
		
		while(num>0) {
			rem=num%10;
			System.out.print(rem);
			num/=10;
		}

	}

}
